
import java.io.*;
import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
        int n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        int[] Arrays=new int[n];
        for(int i=0;i<n;i++)
            Arrays[i]=sc.nextInt();
        int min=65635;
        for(int i=0;i<n;i++)
            {
                if(Arrays[i]<min)
                    min=Arrays[i];
            }
        System.out.println(min);
    }
}
